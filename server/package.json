    "docker-run": "docker run --rm -p 8080:8080 -e DEBUG_MODE_CACHED_POSITIONS=0 --name mybusway-server ${npm_package_custom_packageName}${npm_package_version}",
    "docker-push": "docker push ${npm_package_custom_packageName}${npm_package_version}"
